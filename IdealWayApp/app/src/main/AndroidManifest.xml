<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools" package="com.example.Ideal_play_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:allowBackup="true"
            android:icon="@drawable/logo_2"
            android:label="@string/app_name"
            android:roundIcon="@drawable/logo_2"
            android:supportsRtl="true"
            android:theme="@style/AppTheme" tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.example.Ideal_play_app.RegistrationTwoActivity"></activity>
        <activity android:name="com.example.Ideal_play_app.RegistrationActivity" />
        <activity android:name="com.example.Ideal_play_app.DashboardActivity" />
        <activity android:name="com.example.Ideal_play_app.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.Ideal_play_app.OnboardingActivity" />
        <activity android:name="com.example.Ideal_play_app.QuestionActivity" />
        <activity android:name="com.example.Ideal_play_app.MainActivity" />
    </application>

</manifest>